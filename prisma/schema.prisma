generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Resume {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  userId    String
  title     String
  description String?
  templateId String   @default("modern")
  content   Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([userId])
}
